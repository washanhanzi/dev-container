{
	"name": "MyDevContainer",
	"build": {
		"dockerfile": "Dockerfile",
		"context": ".."
	},
	"runArgs": [
		"-h",
		"dev"
	],
	"customizations": {
		"vscode": {
			"extensions": [
				"rust-lang.rust-analyzer",
				"serayuzgur.crates",
				"vadimcn.vscode-lldb"
			],
			"settings": {
				"editor.formatOnSave": true
			}
		}
	}
}